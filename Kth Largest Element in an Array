class Solution {
    public int findKthLargest(int[] nums, int k) {
        if(nums.length == 0 || nums == null || k == 0) return 0;
        Arrays.sort(nums);
        List<Integer> list = new ArrayList();
        for(int i = nums.length - 1; i>=0; i--){         
                list.add(nums[i]);
            }
        
        return list.get(k - 1);
    }
}
