
//Reverse words in a String

class Solution {
    public String reverseWords(String s) {
        s = s.trim().replaceAll(" +"," ");
        String[] m = s.split(" ");
        String reverse = "";
        for(int i = m.length - 1; i>=0; i--){
            reverse = reverse + m[i] + " ";
        }
        reverse = reverse.trim();
        return reverse;
    }
}